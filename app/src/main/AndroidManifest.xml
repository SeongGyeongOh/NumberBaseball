<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.osg.asmnumberbaseball2">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/baseball"
        android:label="Baseball Game"
        android:roundIcon="@drawable/baseball_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.osg.asmnumberbaseball2.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-9597916099220120~1850671209" />

    </application>

</manifest>